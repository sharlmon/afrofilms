{
  "name": "bezel",
  "version": "1.0.0",
  "main": "gulpfile.js",
  "dependencies": {},
  "devDependencies": {
    "browser-sync": "^2.12.3",
    "css2less": "^0.1.4",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sort": "^2.0.0",
    "gulp-uglify": "^1.5.1",
    "gulp-wp-pot": "^1.1.1"
  },
  "scripts": {
    "clean": "rm -rf ~/Workspace/themes/bezel-wp/bezel-wp/*",
    "sync": "rsync -av --exclude='node_modules' --exclude='.git' --exclude='.gitignore' --exclude='.DS_Store' ./* ~/Workspace/themes/bezel-wp/bezel-wp --delete",
    "clear": "find ~/Workspace/themes/bezel-wp/ -name '*.DS_Store' -type f -delete && find ~/Workspace/themes/bezel-wp/ -name 'bezel-wp.zip' -type f -delete",
    "build": "npm run clean && npm run sync && npm run clear"
  },
